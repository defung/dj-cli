{
  "name": "base-cli",
  "version": "0.0.1",
  "description": "A TypeScript CLI for running commands",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "base-cli": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "tsx src/index.ts",
    "start:compiled": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": ["cli", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "tsx": "^4.15.7",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}